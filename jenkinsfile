pipeline {
    agent any
    parameters{
        string(name:'Env',defaultvalue:'test',description:'Env to deploy')
    }
    stages 
        stage('compile') {
            steps {
                echo 'compile the code'
            }
        }
         stage('unittest') {
            steps {
                echo 'run the test file'
            }
        }
         stage('package') {
            steps {
                echo 'package the code'
            }
        }
    }
}
