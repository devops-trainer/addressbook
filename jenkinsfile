pipeline {
    agent any
    tools{
        jdk 'myjava1'
        maven 'mymaven1'
    }


    stages {
        stage('compile') {
            steps {
                echo "compile the code"
                sh 'mvn compile' 
            }
        }
    stage('UnitTest') {
       
            steps {
                echo 'run the test cases'
                sh 'mvn test'
            }
        }
        stage('package') {
            steps {
                echo "package the code"
                sh 'mvn package'
            }
        }
        
            
        }
    }
}
          
