pipeline {
    agent any
    parameters{
      string(name:'Env',defaultValue='Test',description:'version to deploy')  
    }
    stages {
        stage('compile') {
            steps {
                script{
                echo "Compiling the code"
            }
        }
    }
    stage('Unit Test') {
            steps {
                script{
                echo "Testing the code in env: ${params.Env} env"
            }
        }
    }
    stage('Package') {
            steps {
                script{
                echo "Package the code"
            }
        }
    }
 }
}
