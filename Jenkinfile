pipeline {
    agent any
    stages {
        stage('compile') {
            steps {
                script{
                echo "Compiling the code"
            }
        }
    }
    stage('Unit Test') {
            steps {
                script{
                echo 'Testing the code'
            }
        }
    }
    stage('Package') {
            steps {
                script{
                echo 'Package the code'
            }
        }
    }
 }
}
